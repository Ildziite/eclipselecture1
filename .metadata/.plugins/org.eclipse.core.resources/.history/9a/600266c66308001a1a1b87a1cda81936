public class StaticMethodTask {

	public static void main(String[] args) {
		String argString;
	//	for(String arg: args){
	//		argString=argString+arg;
	//		System.out.println(arg);
	//	}
		for(int i=1;args.length-1;i++){
			argString=argString+arg;
			//		System.out.println(arg);
		}
		System.out.println(argString);
	}
}
