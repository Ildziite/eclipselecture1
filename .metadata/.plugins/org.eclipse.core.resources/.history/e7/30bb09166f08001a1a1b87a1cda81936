class Point {
	int x;
	int y;
	static int MAX_X=10;
	static int MAX_Y=10;
	
	void Point(){
		this(0,0);
	}

	void Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public String toString(){
		return this.x+":"+this.y;
	}
	
}

public class PointTask {

	public static void main(String[] args) {
		Point middlepoint=new Point();
		Point startpoint=new Point();
		Point endpoint=new Point();
		
		

	}

}
