class Pupil {
	int age;
	int form;
	String name;
	public Pupil(){
		this.age=7;
		this.form=1;
	}
	
	public Pupil(String n, int a, int f){
		this.age=a;
		this.form=f;
		this.name=n;
	}
	
	Void output(){
		System.out.println("Name: "+name+" age: "+age+" form: "+form);
	}

}

public class ConstructorExample {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		Pupil.pupil1 = new Pupil();
		
		

	}

}
